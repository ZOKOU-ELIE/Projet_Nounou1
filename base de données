CREATE TABLE UTILISATEUR (ID INT AUTO_INCREMENT PRIMARY KEY, NOM VARCHAR(150), EMAIL VARCHAR(150), MOT_DE_PASSE VARCHAR(150), ROLE ENUM('PARENT', 'NOUNOU'), NUMERO_TELEPHONE VARCHAR(150));
CREATE TABLE ENFANT (ID INT AUTO_INCREMENT PRIMARY KEY, NOM VARCHAR(150), DATE_NAISSANCE DATE, ALLERGIES VARCHAR(150), BESOINS_SPECIAUX VARCHAR(150), ID_PARENT INT, FOREIGN KEY (ID_PARENT) REFERENCES UTILISATEUR(ID));
CREATE TABLE EMPLOIDUTEMPS( ID INT AUTO_INCREMENT PRIMARY KEY, DATE DATE, HEURE_DEBUT TIME, HEURE_FIN TIME, ID_NOUNOU INT, FOREIGN KEY(ID_NOUNOU) REFERENCES UTILISATEUR(ID));
CREATE TABLE ADMINISTRATEUR (ID INT AUTO_INCREMENT PRIMARY KEY, NOM VARCHAR(150),EMAIL VARCHAR(150),MOT_DE_PASSE VARCHAR(150), NUMERO_TELEPHONE VARCHAR(150));
